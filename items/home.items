Dimmer Living_Room_Lamp_Dimmer "Living Room Dimmer [%d %%]" (plugDimmers) {zwave="2"}
Dimmer Kitchen_Lamp_Dimmer "Kitchen Dimmer [%d %%]" (plugDimmers) {zwave="3"}
Switch GreatRoomSwitch "Great Room Switch [%d %%]" {zwave="4:command=SWITCH_MULTILEVEL"}
String GRBASIC "Great Room Switch [%d %%]" {zwave="4:command=BASIC"}
String GRPOWERLEVEL "Great Room Switch [%d %%]" {zwave="4:command=POWERLEVEL"}

/* Use the Astro binding to see when it's dark or light outside. Using a light sensor would be
  even better, but that's not too hard to hack in later */

DateTime dawnEnd "Morning Dawn End [%1$tH:%1$tM]" {astro="planet=sun,type=civilDawn,property=end"}
DateTime duskEnd   "Evening Dusk End [%1$tH:%1$tM]"   {astro="planet=sun,type=civilDusk,property=end"}

/* Device Presence Detection */
Switch someoneIsHome "Someone Is Home!" (devicePresence)
Switch wifi_Kail "Kail Wifi"      (devicePresence)      {nh="192.168.0.206"}
Switch wifi_Andrea "Andrea Wifi"  (devicePresence)      {nh="192.168.0.207"}
