var Number wakeUpDimmer

    wakeUpDimmer=BasementArt.state
    while(wakeUpDimmer>0){
        wakeUpDimmer=wakeUpDimmer-1
        if(wakeUpDimmer < 0) {
          wakeUpDimmer = 0
        }
        sendCommand(BasementArt,wakeUpDimmer)
        Thread::sleep(500)
    }

logDebug("rules", "Done Dimming")
